{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-promise-executor-return": "off",
        "import/no-extraneous-dependencies": "off",
        "default-param-last": "off",
        "import/extensions": "off",
        "import/order": "off",
        "no-console": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "max-len": "off"
    }
}
